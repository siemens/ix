    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.16.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/browser": "^3.2.4",
    "eslint": "~8.21.0",
    "eslint-config-ix": "workspace:*",
    "eslint-plugin-react": "^7.35.0",
    "happy-dom": "^17.4.7",
    "jest": "^29.7.0",
