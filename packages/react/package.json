    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "peerDependencies": {
    "@siemens/ix-icons": "3.0.0-alpha.1",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@siemens/ix": "~3.0.0-alpha.5",
    "@stencil/react-output-target": "^1.0.2",
    "tslib": "*"
  }
