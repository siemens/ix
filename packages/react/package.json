    "react": "^18",
    "react-dom": "^18",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "shadow-dom-testing-library": "^1.11.2",
    "typescript": "^5.6.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "vitest-browser-react": "^1.0.0"
  },
  "peerDependencies": {
    "@siemens/ix-icons": "^3.0.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "@siemens/ix": "~3.1.0",
    "@stencil/react-output-target": "^1.1.0",
    "tslib": "*"
  }
