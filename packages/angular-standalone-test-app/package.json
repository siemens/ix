{
  "name": "angular-standalone-test-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "copy-preview-styles": "node ./scripts/copy-preview.mjs",
    "start": "pnpm copy-preview-styles && ng serve",
    "build": "pnpm copy-preview-styles && ng build",
    "watch": "pnpm copy-preview-styles && ng build --watch --configuration development",
    "test": "playwright test",
    "preview": "http-server ./dist/angular-standalone-test-app/browser -p 4173 --proxy http://localhost:4173?"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "v18-lts",
    "@angular/common": "v18-lts",
    "@angular/compiler": "v18-lts",
    "@angular/core": "v18-lts",
    "@angular/forms": "v18-lts",
    "@angular/platform-browser": "v18-lts",
    "@angular/platform-browser-dynamic": "v18-lts",
    "@angular/router": "v18-lts",
    "@siemens/ix": "workspace:*",
    "@siemens/ix-aggrid": "workspace:*",
    "@siemens/ix-angular": "workspace:*",
    "@siemens/ix-echarts": "workspace:*",
    "@siemens/ix-icons": "3.0.0-alpha.1",
    "ag-grid-angular": "^32.0.2",
    "ag-grid-community": "^32.0.2",
    "echarts": "5.4.3",
    "echarts-gl": "^2.0.9",
    "html-test-app": "workspace:*",
    "ngx-echarts": "^18.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "v18-lts",
    "@angular/cli": "v18-lts",
    "@angular/compiler-cli": "v18-lts",
    "@playwright/test": "^1.49.1",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.16.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.5"
  }
}
