{
  "extends": "@tsconfig/docusaurus/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "ES2022"
    ],
    "noEmit": true,
    "noImplicitAny": false,
    "types": [
      "node",
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ],
    "baseUrl": ".",
    "paths": {
      "@site/*": [
        "./*"
      ]
    },
    "resolveJsonModule": true,
    "preserveSymlinks": true
  },
  "include": [
    "./**/*.ts",
    "./**/*.tsx"
  ]
}
