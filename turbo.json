{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**",
        "www/**",
        "loader/**",
        "component-doc.json"
      ],
      "outputMode": "new-only"
    },
    "start": {
      "cache": false
    }
  }
}