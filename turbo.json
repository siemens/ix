{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "build/**",
        "www/**",
        "loader/**",
        "component-doc.json"
      ],
      "outputMode": "new-only"
    },
    "test": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "test:e2e": {
      "cache": false
    }
  }
}
